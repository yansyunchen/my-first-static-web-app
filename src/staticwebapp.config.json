{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/anonymous/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authenticated/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/custom/*",
      "allowedRoles": ["custom"]
    }
  ]
}
